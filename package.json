{
  "name": "pinephone",
  "type": "module",
  "version": "0.1.2",
  "description": "JavaScript library which provides a consistent API for fetching device info and sensor data from Pine64 PinePhone devices",
  "main": "./build/pinephone.cjs",
  "module": "./build/pinephone.module.js",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "import": "./build/pinephone.module.js",
      "require": "./build/pinephone.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c utils/build/rollup.config.js",
    "postbuild": "node npm-scripts/post-build.mjs",
    "build-legacy": "rollup ./src/pinephone.js --file ./build/pinephone.js --format cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BraidenPsiuk/pinephone.js.git"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "connectivity",
    "node-js",
    "pine64",
    "pinephone",
    "mobian",
    "manjaro-arm",
    "pinephone-pro"
  ],
  "author": "BRAIDEN S. PSIUK <mail@braiden.dev> (https://braiden.dev)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/BraidenPsiuk/pinephone.js/issues"
  },
  "homepage": "https://github.com/BraidenPsiuk/pinephone.js#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "rollup": "^3.2.3"
  }
}
